{
  "name": "molovo/laravel-model-traits",
  "description": "A collection of traits for use with Laravel 5 models.",
  "keywords": [
    "molovo",
    "laravel",
    "traits",
    "eloquent"
  ],
  "homepage": "https://github.com/molovo/laravel-model-traits",
  "license": "MIT",
  "authors": [ {
    "name": "James Dinsdale",
    "email": "hi@molovo.co",
    "homepage": "https://molovo.co",
    "role": "Developer"
  } ],
  "require": {
    "php": ">=5.5.9"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Molovo\\ModelTraits\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Molovo\\ModelTraits\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "post-update-cmd": [
      "php artisan vendor:publish --tag=public --force"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}